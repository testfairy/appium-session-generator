{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "npm run build-node",
    "build-node": "rm -rf dist-node; tsdx build --target node && mv dist dist-node && cp -R template dist-node && cp src/cli.js dist-node",
    "test": "npm run test-node",
    "test-node": "export TF_BROWSER_TEST=0 && tsdx test; unset TF_BROWSER_TEST",
    "lint": "tsdx lint",
    "prepare": "tsdx build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "useTabs": false,
    "overrides": [
      {
        "files": "*.js",
        "options": {
          "semi": true
        }
      }
    ]
  },
  "name": "appium-session-generator",
  "author": "Diego Perini",
  "module": "dist/appium-session-generator.esm.js",
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/node-static": "^0.7.5",
    "@types/opener": "^1.4.0",
    "@types/server-destroy": "^1.0.0",
    "browserify": "^16.5.1",
    "husky": "^4.2.5",
    "node-static": "^0.7.11",
    "opener": "^1.5.1",
    "server-destroy": "^1.0.1",
    "tsdx": "^0.13.2",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/mustache": "^4.0.1",
    "file-saver": "^2.0.2",
    "getopts": "^2.2.5",
    "jszip": "^3.4.0",
    "mustache": "^4.0.1",
    "request": "^2.88.2",
    "uuid": "^8.2.0"
  }
}
